  "keywords": [
    "baidu",
    "netdisk",
    "sdk"
  ],
  "author": "keenghost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keenghost/BaiduNetdiskNode.git",
    "directory": "packages/sdk"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "^9.0.0"
